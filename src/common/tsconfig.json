{
  "compilerOptions": {
    "module": "commonjs",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "strictNullChecks": true,
    "pretty": true,
    "target": "es5",
    "lib": ["es5", "es2015.promise"],
    "sourceMap": true
  },
  "exclude": [
    "node_modules"
  ]
}